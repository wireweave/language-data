{
  "name": "@wireweave/language-data",
  "version": "1.3.0-beta.0",
  "description": "Shared language definitions for Wireweave DSL editors",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./monaco": {
      "types": "./dist/monaco/index.d.ts",
      "import": "./dist/monaco/index.js",
      "require": "./dist/monaco/index.cjs"
    },
    "./codemirror": {
      "types": "./dist/codemirror/index.d.ts",
      "import": "./dist/codemirror/index.js",
      "require": "./dist/codemirror/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.4",
    "release-it": "^19.2.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.1.1"
  },
  "keywords": [
    "wireframe",
    "dsl",
    "language",
    "intellisense",
    "editor"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wireweave/language-data.git"
  }
}
